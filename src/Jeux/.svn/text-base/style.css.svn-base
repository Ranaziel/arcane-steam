.lnk_user{
	width:450px;
	height:30px;
	text-decoration:none;
	font-size:18px;
	color:#ff9900;
	padding:2px 0px 2px 0px;
	margin:2px 0px 2px 0px;
}

.lnk_user:hover{
	width:450px;
	height:30px;
	text-decoration:none;
	font-size:18px;
	background:#999999;
	color:#C0C0C0;
	border:solid 1px #C0C0C0;
	padding:2px 0px 2px 0px;
	margin:2px 0px 2px 0px;
}

.lnk_user img{
	border:none;
	text-decoration:none;
}




.lnk_mini{
	text-decoration:none;
	font-size:9px;
	color:#ff9900;
	padding-right:2px;
	padding-left:2px;
	vertical-align:middle;
}

.lnk_mini:hover{
	text-decoration:none;
	font-size:9px;
	color:#C0C0C0;
	border:solid 1px #C0C0C0;
	padding-right:2px;
	padding-left:2px;
	vertical-align:middle;
}

.lnk_mini img{
	border:none;
	text-decoration:none;
	vertical-align:middle;
	padding-left:2px;
	padding-right:5px;
	margin-bottom:2px;
}
